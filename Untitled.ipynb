   "source": [
    "# write a program to sort the numbers\n",
    "nums = [5, 4, 1, 7, 8, 2]\n",
    "for i in range(len(nums)):\n",
    "  for j in range(i+1, len(nums)):\n",
    "    small = nums[i]\n",
    "    if small > nums[j]:\n",
    "      nums[i] = nums[j]\n",
    "      nums[j] = small\n",
    "\n",
    "print(nums)\n",
    "\n",
    "    "
   ]
